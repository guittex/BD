	height: 16px;
}

table {
	border-collapse: collapse;
	border-spacing: 0px;
}

table.td {
	padding: 0px;
}

input.button {
	border-radius: 5px 5px 5px 5px;
	border-top: 1px solid rgb(186, 186, 186);
	border-left: 1px solid rgb(148, 148, 148);
	border-right: 1px solid rgb(148, 148, 148);
	border-bottom: 1px solid rgb(97, 97, 97);
	background: rgb(247, 247, 247);
	font-weight: bold;
	padding-bottom: 4px;
	height: 22px;
}

input.button:hover {
	border-radius: 5px 5px 5px 5px;
	border-top: 1px solid rgb(2, 155, 255);
	border-left: 1px solid rgb(0, 135, 216);
	border-right: 1px solid rgb(0, 135, 216);
	border-bottom: 1px solid rgb(0, 121, 181);
	background: rgb(250, 252, 247);
	font-weight: bold;
	padding-bottom: 4px;
	height: 22px;
}

input.button:focus {
	border-radius: 5px 5px 5px 5px;
	border-top: 1px solid rgb(2, 155, 255);
	border-left: 1px solid rgb(0, 135, 216);
	border-right: 1px solid rgb(0, 135, 216);
	border-bottom: 1px solid rgb(0, 121, 181);
	background: rgb(181, 226, 255);
	font-weight: bold;
	padding-bottom: 4px;
	height: 22px;
}

.grid {
    width: 100%;
    border-left: 1px solid #CCCCCC;
    border-bottom: 1px solid #CCCCCC;
}
.grid_th_image_down {
	background: #7FCEFF;
}
.grid_th_image_over {
	background: #B2E1FF;
}
.grid_th_image {
	background: url(../imagens/grid_bg.jpg);	
}
.grid_th {
	empty-cells: show;
	height: 12px;
	padding: 4px;
	text-align: left;
	font-weight: bold;
    border-top: 1px solid #CCCCCC;
	border-right: 1px solid #CCCCCC;
	border-bottom: 1px solid #CCCCCC;
}
.grid_td {
	height: 12px;
	empty-cells: show;
    border-right: 1px solid #CCCCCC;
	padding: 4px;
}
.grid_td_down {
/*	background-color: rgb(77, 178, 240);*/
	background-color: #7FCEFF;
}
.grid_td_over {
/*	background-color: rgb(129, 192, 231);*/
	background-color: #B2E1FF;
}
.grid_par {
	background-color: #F7F7F7;
}
.grid_impar {
    background-color: #FFFFFF;
}

