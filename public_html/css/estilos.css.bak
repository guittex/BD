
* {
	font-family: Verdana;
	font-size: 11px;
	color: rgb(11, 51, 60);
	cursor: default;
}

body {
	margin: 0px;
}

.bold {
	font-weight: bold;
}

.destaque {
	font-weight: bold;
	color: #FF0064;
}

