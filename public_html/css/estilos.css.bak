.tab_over_checked {
	border-left: 1px solid rgb(150, 150, 150);
	border-top: 1px solid rgb(150, 150, 150);
	border-right: 1px solid rgb(150, 150, 150);
	border-collapse: separate;
	border-radius: 8px 32px 0px 0px;
	-moz-border-radius: 8px 32px 0px 0px;
	background: #7FCEFF;
	height: 19px;
}

.tab_over_unchecked {
	border-left: 1px solid rgb(150, 150, 150);
	border-top: 1px solid rgb(150, 150, 150);
	border-right: 1px solid rgb(150, 150, 150);
	border-collapse: separate;
	border-radius: 8px 32px 0px 0px;
	-moz-border-radius: 8px 32px 0px 0px;
	background: #B2E1FF;
	height: 18px;
}

.tab_checked {
	border-left: 1px solid rgb(150, 150, 150);
	border-top: 1px solid rgb(150, 150, 150);
	border-right: 1px solid rgb(150, 150, 150);
	border-collapse: separate;
	border-radius: 8px 32px 0px 0px;
	-moz-border-radius: 8px 32px 0px 0px;
	background: #7FCEFF;
	height: 19px;
}

.tab_unchecked {
	border-left: 1px solid rgb(150, 150, 150);
	border-top: 1px solid rgb(150, 150, 150);
	border-right: 1px solid rgb(150, 150, 150);
	border-collapse: separate;
	border-radius: 8px 32px 0px 0px;
	-moz-border-radius: 8px 32px 0px 0px;
	background: url(../imagens/tab_bg.png);
	height: 18px;
}

.title {
	font-size: 18pt;
	font-weight: bold;
	text-align: center;
}

hr {
	border: 1px solid #CCCCCC;
}

.paginacao_item_selected {
	width: 20px;
	height: 20px;
	border-left: 1px solid #7DA2CE;
	border-right: 1px solid #7DA2CE;
	background: url(../imagens/item.bg.down.png);
	text-align: center;
}

.paginacao_item_over {
	width: 20px;
	height: 20px;
	border-left: 1px solid #B8D6FB;
	border-right: 1px solid #B8D6FB;
	background: url(../imagens/item.bg.over.png);
	text-align: center;
}

.paginacao_item {
	width: 20px;
